#!/bin/bash
PREFIX=$1
NAME=$2
cat <<EOF
#!/bin/bash
. ./authn
ADDL=\$1
if [[ -z "\${ADDL}" ]] ; then
   ADDL="--header,Accept: application/json"
fi
IFS=',' read -r -a array <<< "\$ADDL"
curl \${OPT} -s -k -H "\$AUTHN" \\
   -X GET \\
   "\${array[@]}" \\
   "https://\${RESTHOST}:\${RESTPORT}${PREFIX}${NAME}"
EOF
