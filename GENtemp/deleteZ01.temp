#!/bin/bash
PREFIX=$1
NAME=$2
cat <<EOF
#!/bin/bash
. ./authn
NAME=\$1
ADDL=\$2
if [[ -z "\${ADDL}" ]]; then
   ADDL="--header,Accept: application/json"
fi
IFS=',' read -r -a array <<< "\$ADDL"
curl \${OPT} -s -k -H "\$AUTHN" \\
   -X DELETE \\
   "\${array[@]}" \\
   "https://\${RESTHOST}:\${RESTPORT}${PREFIX}${NAME}/\${NAME}"
EOF
